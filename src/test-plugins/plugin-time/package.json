{
  "name": "@elizaos/plugin-time",
  "version": "1.0.0",
  "description": "Provides current time and timezone information",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "dev": "tsup --watch"
  },
  "dependencies": {
    "@elizaos/core": "^1.0.0",
    "dayjs": "^1.11.10",
    "timezone-support": "^3.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/coverage-v8": "^1.6.1",
    "eslint": "^8.56.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "elizaos": {
    "actions": [
      "getCurrentTime",
      "convertTime"
    ],
    "providers": [
      "timeProvider"
    ]
  }
}
